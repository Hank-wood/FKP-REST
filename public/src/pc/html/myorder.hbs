<!-- 弹出框 start -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
   <div class="modal-dialog"><!-- 登录框 start -->
      <div class="modal-content">
         <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                 &times;
            </button>
            <h4 class="modal-title" id="myModalLabel">登录</h4>
         </div>
         <div class="modal-body login_body">
            <form method="post">
                <div class="login_input">
                  <input class="user_phone form-control" placeholder="手机号" type="text">
                  <input class="user_pwd form-control" placeholder="密码" type="password">
                  <div class="l_code clearfix hidden">
                      <input class="code fl form-control" placeholder="验证码" type="text">
                      <img src="statics/front/images/yzm.png"/>
                      <a href="#">换一张</a>
                  </div>
                </div>
                <div class="login_tip clear">
                  <div class="login_remeber fl"><b class="checkbox"></b><span class="m_t5 fl">下次自动登录</span></div>
                  <div class="login_password fr m_t5"><a href="forget.html">忘记密码？</a></div>
                </div>
                <div class="login_btn"><button type="button" class="btn btn-primary">登录</button></div>
                <div class="login_reg"><a href="#">立即注册</a></div>
            </form>
         </div>
      </div><!-- 登录框 end -->
    </div>
</div><!-- 弹出框 end -->
<!-- 弹出框 start -->
<div class="modal fade" id="myModalmerchant" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
   <div class="modal-dialog"><!-- 商家详情 start -->
      <div class="modal-content">
         <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                 &times;
            </button>
            <h4 class="modal-title" id="myModalLabel">已认证会员</h4>
         </div>
         <div class="modal-body merchant_body">
            <form method="post">
                <div class="login_input">
                    <form role="form">
                       <div class="form-group">
                          <label for="name">用户名：</label>
                          <input type="text" class="form-control user_name" placeholder="lgh">
                       </div>
                       <div class="form-group">
                          <label for="name">企业名称：</label>
                          <input type="text" class="form-control user_company" placeholder="lgh公司">
                       </div>
                       <div class="form-group">
                          <label for="name">联系人：</label>
                          <input type="text" class="form-control user_tname" placeholder="lgh">
                       </div>
                       <div class="form-group">
                          <label for="name">联系电话：</label>
                          <input type="text" class="form-control user_phone" placeholder="111111000">
                       </div>
                       <div class="form-group">
                          <label for="name">传真号码：</label>
                          <input type="text" class="form-control user_fax" placeholder="1111111000">
                       </div>
                       <div class="form-group">
                          <label for="name">联系邮箱：</label>
                          <input type="text" class="form-control user_email" placeholder="110@qq.com">
                       </div>
                       <div class="form-group">
                          <label for="name">联系QQ：</label>
                          <input type="text" class="form-control user_qq" placeholder="110">
                       </div>
                        <div class="form-group u_address">
                            <label for="name">地区：</label>
                            <select class="form-control user_asheng">
                                <option>广东省</option>
                                <option>广西省</option>
                                <option>广南省</option>
                                <option>江西省</option>
                                <option>湖南省</option>
                            </select>
                            <select class="form-control user_ashi">
                                <option>广州市</option>
                                <option>佛山市</option>
                                <option>汕尾市</option>
                                <option>深圳市</option>
                                <option>汕头市</option>
                            </select>
                            <select class="form-control user_aqu">
                                <option>天河区</option>
                                <option>白云区</option>
                                <option>越秀区</option>
                                <option>黄埔区</option>
                                <option>海珠区</option>
                            </select>
                            <input type="text" class="form-control user_adetail"/>
                        </div>
                    </form>
                </div>
            </form>
         </div>
      </div><!-- 登录框 end -->
    </div>
</div><!-- 弹出框 end -->
<script type="text/javascript" src="statics/front/js/jquery-1.8.3.min.js"></script>
