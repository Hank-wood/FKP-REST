{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": false,
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "isolatedModules": false,
    "experimentalDecorators": false,
    "emitDecoratorMetadata": false,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": false,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "noEmitHelpers": false,
    "allowSyntheticDefaultImports": false,
    "allowJs": true,
    "moduleResolution": "classic",
    "rootDir": ".",
    "watch": true,
    "newLine": "LF"
  },
  "awesomeTypescriptLoaderOptions": {},
  "filesGlob": [
    "./**/*.ts",
    "./**/*.tsx",
    "!./react/**/*",
    "!./node_modules/**/*"
  ],
  "files": [
    "./typings/index.d.ts",
    "./src/pc/js/pages/tpagi/index.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "atom": {
    "rewriteTsconfig": false
  }
}
